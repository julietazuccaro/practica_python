   "source": [
    "def wrap(string, max_width):\n",
    "    n = len(string)\n",
    "    partes  = [string[i:i + max_width] for i in range(0, n, max_width)]\n",
    "    partes = print(\"\\n\".join(partes))\n",
    "    return partes\n",
    "    "
   ]
